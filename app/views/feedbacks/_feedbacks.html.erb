<% @feedbacks.each do |feedback| %>

  <div class="card mb-4 shadow-sm" >
    <div class="container" >
      <h2><%= feedback.title %></h2>
      <p><%= feedback.body %></p>
      <%= link_to 'Delete', feedback_path(feedback),
                  method: :delete,
                  data: {confirm: 'Are you sure?'},
                  :class => 'btn btn-sm btn-secondary',
                  :type => 'button' %>
      <p></p>
    </div>
  </div>

<% end %>

